//Cubing numbers, and if it's odd, sum it to the result

public class FindOddCubes {
 
public static int cubeOdd(int arr[]) {
        int result = 0;
        
        // Loop through the array
        for (int i = 0; i < arr.length; i++) {

            // Cube the number
            int cube = arr[i] * arr[i] * arr[i];
            
            // Check if the cube is odd, if so, add it to the result
            if (cube % 2 != 0) {
                result += cube;
            }
        }

        return result;
    }
}
